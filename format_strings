#f-string
age = 36
txt = f"My name is John, I am {age}"
print(txt)

#placeholders
price = 59
txt = f"The price is {price} dollars"
print(txt)

#modifier inside a placeholder
price = 59
txt = f"The price is {price:.2f} dollars"
print(txt)

#placeholder with a math operation
txt = f"The price is {20 * 59} dollars"
print(txt)